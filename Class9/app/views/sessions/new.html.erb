<div id = "login">
		
		<h1><center><u>Log In</u></center></h1>

		<%=form_tag login_path do%>
		<%=label_tag :email %>
		<%=text_field_tag :email %>

		<%=label_tag :password %>
		<%=password_field_tag :password %>
		<%=submit_tag "Log in" %>
			<%end%>
</div>
<br>

<%= link_to "Sign-up", new_user_path %>