<!DOCTYPE html>
<html>
<head>
  <title>Fourthapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag('//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css', :media => 'all') %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag '//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<body class="pleasant">

<%= flash[:notice] if flash[:notice] %> 
<%= flash[:alert] if flash[:alert] %>
<div class="custom-container">
<%= yield %>
</div>

</body>

</html>
